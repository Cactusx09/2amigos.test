{
  "version": 2,
  "env": {
    "ON_VERCEL": "true"
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}
